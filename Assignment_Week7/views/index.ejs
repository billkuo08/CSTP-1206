<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>Document</title>
</head>

<body>
    <h1>HotelsPro</h1>

    <P>Congratulation</P>
    <p>
        <% for ( let key in userData) { %>
            <% if (key !=="socials" ) { %>
                <p>
                    <%- userData[key] %>
                </p>
                <% } %>
                    <% } %>

                        <% for ( let link in userData.socials) { %>

                            <a href=<%- userData.socials[link] %>>
                                <img src=<%- link %>
                                    width="50px" height="50px">

                            </a>
                         
                            <% } %>

    </p>


</body>

</html>